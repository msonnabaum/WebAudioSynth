  background-image: -moz-radial-gradient(50% 0%, 8% 50%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%), -moz-radial-gradient(50% 100%, 12% 50%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%), -moz-radial-gradient(0% 50%, 50% 7%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%), -moz-radial-gradient(100% 50%, 50% 5%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%);
  background-image: -ms-radial-gradient(50% 0%, 8% 50%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%), -ms-radial-gradient(50% 100%, 12% 50%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%), -ms-radial-gradient(0% 50%, 50% 7%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%), -ms-radial-gradient(100% 50%, 50% 5%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%);
  background-image: -o-radial-gradient(50% 0%, 8% 50%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%), -o-radial-gradient(50% 100%, 12% 50%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%), -o-radial-gradient(0% 50%, 50% 7%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%), -o-radial-gradient(100% 50%, 50% 5%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%);
  background-image: radial-gradient(50% 0%, 8% 50%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%), radial-gradient(50% 100%, 12% 50%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%), radial-gradient(0% 50%, 50% 7%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%), radial-gradient(100% 50%, 50% 5%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 100%); }

