body {
  background: #333;
  color: #DDD;
  width: 1640px; }

#keyboard {
  background: #222;
  padding-left: 50px;
  width: 98%;
  overflow: hidden;
  z-index: -100;
  padding: 10px;
  display: -webkit-box;
  display: -moz-box;
  display: box;
  position: absolute; }

#keys-playable {
  width: 640px;
  border: 10px #000 solid; }

#keys-wrapper {
  display: -webkit-box;
  display: -moz-box;
  display: box;
  /*margin-left: -675px;*/
  margin-left: -1290px; }

.key {
  position: relative;
  display: -moz-inline-box;
  -moz-box-orient: vertical;
  display: inline-block;
  vertical-align: baseline;
  zoom: 1;
  *display: inline;
  *vertical-align: auto; }

.white {
  -webkit-transition-property: all;
  -moz-transition-property: all;
  -ms-transition-property: all;
  -o-transition-property: all;
  transition-property: all;
  -webkit-transition-duration: 0.1s;
  -moz-transition-duration: 0.1s;
  -ms-transition-duration: 0.1s;
  -o-transition-duration: 0.1s;
  transition-duration: 0.1s;
  -webkit-transition-timing-function: ease-in-out;
  -moz-transition-timing-function: ease-in-out;
  -ms-transition-timing-function: ease-in-out;
  -o-transition-timing-function: ease-in-out;
  transition-timing-function: ease-in-out;
  -webkit-transition-delay: 0;
  -moz-transition-delay: 0;
  -ms-transition-delay: 0;
  -o-transition-delay: 0;
  transition-delay: 0;
  width: 60px;
  height: 200px;
  background: #fff;
  float: left;
  border-right: 1px solid #666;
  -webkit-box-shadow: 0 10px 2px 0 rgba(0, 0, 0, 0.15);
  -moz-box-shadow: 0 10px 2px 0 rgba(0, 0, 0, 0.15);
  -ms-box-shadow: 0 10px 2px 0 rgba(0, 0, 0, 0.15);
  -o-box-shadow: 0 10px 2px 0 rgba(0, 0, 0, 0.15);
  box-shadow: 0 10px 2px 0 rgba(0, 0, 0, 0.15);
  -webkit-border-radius: 6px;
  -moz-border-radius: 6px;
  -ms-border-radius: 6px;
  -o-border-radius: 6px;
  border-radius: 6px;
  margin-left: -20px;
  z-index: -1; }

.black {
  -webkit-transition-property: all;
  -moz-transition-property: all;
  -ms-transition-property: all;
  -o-transition-property: all;
  transition-property: all;
  -webkit-transition-duration: 0.1s;
  -moz-transition-duration: 0.1s;
  -ms-transition-duration: 0.1s;
  -o-transition-duration: 0.1s;
  transition-duration: 0.1s;
  -webkit-transition-timing-function: ease-in-out;
  -moz-transition-timing-function: ease-in-out;
  -ms-transition-timing-function: ease-in-out;
  -o-transition-timing-function: ease-in-out;
  transition-timing-function: ease-in-out;
  -webkit-transition-delay: 0;
  -moz-transition-delay: 0;
  -ms-transition-delay: 0;
  -o-transition-delay: 0;
  transition-delay: 0;
  width: 40px;
  height: 120px;
  margin-left: -20px;
  z-index: 2;
  background-color: #3b3b3d;
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #3b3b3d), color-stop(81%, #4d4d54));
  background-image: -webkit-linear-gradient(top, #3b3b3d 0%, #4d4d54 81%);
  background-image: -moz-linear-gradient(top, #3b3b3d 0%, #4d4d54 81%);
  background-image: -ms-linear-gradient(top, #3b3b3d 0%, #4d4d54 81%);
  background-image: -o-linear-gradient(top, #3b3b3d 0%, #4d4d54 81%);
  background-image: linear-gradient(top, #3b3b3d 0%, #4d4d54 81%);
  float: left;
  border-right: 1px solid #666;
  -webkit-box-shadow: inset 0 -10px 3px 5px black;
  -moz-box-shadow: inset 0 -10px 3px 5px black;
  -ms-box-shadow: inset 0 -10px 3px 5px black;
  -o-box-shadow: inset 0 -10px 3px 5px black;
  box-shadow: inset 0 -10px 3px 5px black; }

.black::after {
  content: "";
  position: absolute;
  bottom: 2px;
  left: 14%;
  z-index: 3;
  width: 70%;
  height: 9%;
  background: #39393b;
  -webkit-box-shadow: inset 0 5px 7px -5px #0a0a0a;
  -moz-box-shadow: inset 0 5px 7px -5px #0a0a0a;
  -ms-box-shadow: inset 0 5px 7px -5px #0a0a0a;
  -o-box-shadow: inset 0 5px 7px -5px #0a0a0a;
  box-shadow: inset 0 5px 7px -5px #0a0a0a;
  -webkit-border-radius: 5px 5px 3px 3px;
  -moz-border-radius: 5px 5px 3px 3px;
  -ms-border-radius: 5px 5px 3px 3px;
  -o-border-radius: 5px 5px 3px 3px;
  border-radius: 5px 5px 3px 3px;
  border: #000 solid 1px; }

.white:hover, .white-down {
  -webkit-box-shadow: 0 6px 2px rgba(0, 0, 0, 0.15);
  -moz-box-shadow: 0 6px 2px rgba(0, 0, 0, 0.15);
  -ms-box-shadow: 0 6px 2px rgba(0, 0, 0, 0.15);
  -o-box-shadow: 0 6px 2px rgba(0, 0, 0, 0.15);
  box-shadow: 0 6px 2px rgba(0, 0, 0, 0.15);
  border-radius: 5px;
  background-color: #dedede;
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #dedede), color-stop(9%, #e3e3e3), color-stop(100%, #f7f7f7));
  background-image: -webkit-linear-gradient(center bottom, #dedede 0%, #e3e3e3 9%, #f7f7f7 100%);
  background-image: -moz-linear-gradient(center bottom, #dedede 0%, #e3e3e3 9%, #f7f7f7 100%);
  background-image: -ms-linear-gradient(center bottom, #dedede 0%, #e3e3e3 9%, #f7f7f7 100%);
  background-image: -o-linear-gradient(center bottom, #dedede 0%, #e3e3e3 9%, #f7f7f7 100%);
  background-image: linear-gradient(center bottom, #dedede 0%, #e3e3e3 9%, #f7f7f7 100%); }

.black:hover, .black-down {
  /*
  width: 40px;
  height: 120px;
  */
  margin-left: -20px;
  z-index: 2;
  border-right: 1px solid #666;
  background-color: #3b3b3d;
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #3b3b3d), color-stop(81%, #4d4d54));
  background-image: -webkit-linear-gradient(top, #3b3b3d 0%, #4d4d54 81%);
  background-image: -moz-linear-gradient(top, #3b3b3d 0%, #4d4d54 81%);
  background-image: -ms-linear-gradient(top, #3b3b3d 0%, #4d4d54 81%);
  background-image: -o-linear-gradient(top, #3b3b3d 0%, #4d4d54 81%);
  background-image: linear-gradient(top, #3b3b3d 0%, #4d4d54 81%);
  -webkit-box-shadow: inset 0 -1px 3px 5px black;
  -moz-box-shadow: inset 0 -1px 3px 5px black;
  -ms-box-shadow: inset 0 -1px 3px 5px black;
  -o-box-shadow: inset 0 -1px 3px 5px black;
  box-shadow: inset 0 -1px 3px 5px black; }

.black::after:hover, .black-down::after {
  content: "";
  position: absolute;
  bottom: 2px;
  left: 14%;
  z-index: 3;
  width: 70%;
  height: 0;
  background: #39393b;
  border: #000 solid 1px;
  -webkit-border-radius: 5px 5px 3px 3px;
  -moz-border-radius: 5px 5px 3px 3px;
  -ms-border-radius: 5px 5px 3px 3px;
  -o-border-radius: 5px 5px 3px 3px;
  border-radius: 5px 5px 3px 3px;
  -webkit-box-shadow: inset 0 5px 10px -5px #0a0a0a;
  -moz-box-shadow: inset 0 5px 10px -5px #0a0a0a;
  -ms-box-shadow: inset 0 5px 10px -5px #0a0a0a;
  -o-box-shadow: inset 0 5px 10px -5px #0a0a0a;
  box-shadow: inset 0 5px 10px -5px #0a0a0a; }

.cf {
